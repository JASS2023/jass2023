<launch>
	<arg name="node_name" value="led_signal_node"/>
	<arg name="veh" doc="Name of vehicle. ex: megaman"/>
<!--   <arg name="param_file_name" default="LED_protocol" doc="Specify a param file. ex:megaman"/> -->
	<arg name="robot_type" default="duckiebot" doc="Type of robot"/>

	<group ns="$(arg veh)">
	    <node pkg="led_signal" type="led_signal_node.py"  name="$(arg node_name)" output="screen" >
<!-- 			<rosparam command="load" file="$(find led_signal)/config/$(arg node_name)/$(arg param_file_name).yaml"/> -->
			<param name="robot_type" value="$(arg robot_type)"/>
		</node>
	</group>

</launch>
